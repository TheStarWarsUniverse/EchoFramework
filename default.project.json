{
  "name": "EchoFramework",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$ignoreUnknownInstances": true,

      "Echo": {
        "$path": "src/EchoShared",

        "Packages": {
          "$path": "Packages"
        }
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$ignoreUnknownInstances": true,

      "Echo": {
        "$path": "src/EchoServer",

        "ServerPackages": {
          "$path": "ServerPackages"
        }
      },

      "ServerRuntime": {
        "$path": "src/Runtimes/Server"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "$ignoreUnknownInstances": true,

      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$ignoreUnknownInstances": true,

        "Echo": {
          "$path": "src/EchoClient"
        },
        
        "ClientRuntime": {
          "$path": "src/Runtimes/Client"
        }
      }
    }
  }
}